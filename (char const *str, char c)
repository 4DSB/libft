#include <libft.h>

static int		ft_isupper(int c)
{
	return (c <= 'Z' && c >= 'A');
}

static int		ft_islower(int c)
{
	return (c <= 'z' && c >= 'a');
}

int				ft_isalpha(int c)
{
	return (ft_islower(c) || ft_isupper(c));
}
