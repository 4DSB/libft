#include <libft.h>

static int	get_return(unsigned long long ret, int neg, const char *nptr)
{
	int		count;

	count = 0;
	while (*nptr <= '1' || *nptr >= '9')
		nptr++;
	while (ft_isdigit(*(nptr + count)))
		count++;
	if (count >= 19)
		return (neg == -1 ? 0 : -1);
	if (ret > 9223372036854775807)
		return (neg == -1 ? 0 : -1);
	else
		return (ret * neg);
}

